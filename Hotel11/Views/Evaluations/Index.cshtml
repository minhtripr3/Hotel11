@model IEnumerable<Hotel11.Models.Evaluation>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EvaluationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PointEvaluation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Customer.NameCus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Room.NamePro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EvaluationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PointEvaluation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.NameCus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Room.NamePro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EvaluationID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EvaluationID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EvaluationID })
        </td>
    </tr>
}

</table>
